public class Main
{
       public static void main(String[] args) {
           int arr[]={11,27,38,49,51};
           int n=arr.length;
           int sum=0;
           int avg=0;
           for(int i=0;i<n;i++){
               sum=sum+arr[i];
               avg=sum/n;
           }
           int c=avg;
           System.out.print("avg="+c);
       }
}
