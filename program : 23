public class twodarry {
    public static void main(String[]args) {
        int[] []a={{1,2,3,4},
                   {3,2,4,2},
                   {8,9,4,0},
                   {2,5,8,1}};
                   for(int i=0;i<a.length;i++){
                       for(int j=0;j<a.length;j++){
                           System.out.print(a[i][j]+"\t");
                       }
                }
        }
}
